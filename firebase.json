{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/register",
        "destination": "/register.html"
      },
      {
        "source": "/adatkezelesitajekoztato",
        "destination": "/adatkezelesitajekoztato.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
